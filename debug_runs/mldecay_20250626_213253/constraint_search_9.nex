#NEXUS
begin paup;
execute alignment.nex;
    lset nthreads=12 nst=1 basefreq=equal rates=equal pinvar=0;
    set criterion=likelihood;
constraints clade_constraint (MONOPHYLY) = ((Macaca_fuscata, Macaca_mulatta, Macaca_fascicularis, Macaca_sylvanus));
set maxtrees=100 increase=auto;
hsearch start=stepwise addseq=random nreps=1;
hsearch start=1 enforce=yes converse=yes constraints=clade_constraint;
savetrees file=constraint_tree_9.tre format=newick brlens=yes replace=yes;
lscores 1 / scorefile=constraint_score_9.txt replace=yes;
quit;
end;
