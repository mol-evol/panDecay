#NEXUS
begin paup;
execute alignment.nex;
    lset nthreads=14 nst=1 basefreq=empirical rates=equal pinvar=0;
    set criterion=likelihood;
bootstrap nreps=100 search=heuristic keepall=no treefile=bootstrap_trees.tre replace=yes / start=stepwise addseq=random nreps=1;
savetrees file=bootstrap_trees.tre format=newick brlens=yes replace=yes supportValues=nodeLabels;
quit;
end;
