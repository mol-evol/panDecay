#NEXUS
begin paup;
execute alignment.nex;
    lset nthreads=14 nst=1 basefreq=empirical rates=equal pinvar=0;
    set criterion=likelihood;
gettrees file=ml_tree.tre mode=3 storebrlens=yes;
gettrees file=constraint_tree_3.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_4.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_5.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_6.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_7.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_8.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_9.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_10.tre mode=7 storebrlens=yes;
gettrees file=constraint_tree_11.tre mode=7 storebrlens=yes;
treeinfo;
lscores 1-10 / scorefile=all_tree_scores.txt replace=yes;
lscores 1-10 / autest=yes scorefile=au_test_results.txt replace=yes;
log file=au_test_detailed.log replace=yes;
describe 1-10 / plot=none;
lscores 1-10;
log stop;
quit;
end;
